{
  "name": "datocms-plugin-oembed",
  "version": "0.1.0",
  "datoCmsPlugin": {
    "title": "OEmbed (embed anything)",
    "previewImage": "",
    "coverImage": "",
    "entryPoint": "build/index.html"
  },
  "scripts": {
    "dev": "npm start",
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "datocms-plugin-sdk": "^1.0.0",
    "datocms-react-ui": "^1.0.0",
    "lodash": "^4.17.21",
    "oembed-providers": "^1.0.20230906",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^18.19.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/lodash": "^4.14.202",
    "cross-env": "^7.0.3"
  }
}
