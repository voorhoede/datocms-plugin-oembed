{
  "name": "datocms-plugin-oembed",
  "version": "0.2.0",
  "description": "DatoCMS plugin to get embed data for external content, using the OEmbed protocol.",
  "keywords": [
    "datocms-plugin",
    "embed",
    "oembed",
    "og",
    "opengraph",
    "open-graph",
    "metadata"
  ],
  "homepage": "https://github.com/voorhoede/datocms-plugin-oembed/tree/main/packages/oembed-plugin#readme",
  "author": "De Voorhoede (https://voorhoede.nl/)",
  "datoCmsPlugin": {
    "title": "OEmbed (embed anything)",
    "previewImage": "docs/datocms-plugin-oembed-twitter.mov",
    "coverImage": "docs/datocms-plugin-oembed-cover.jpg",
    "entryPoint": "build/index.html",
    "permissions": []
  },
  "files": [
    "build",
    "docs",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "npm start",
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "datocms-plugin-sdk": "^1.0.0",
    "datocms-react-ui": "^1.0.0",
    "lodash": "^4.17.21",
    "oembed-providers": "^1.0.20230906",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^18.19.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/lodash": "^4.14.202",
    "cross-env": "^7.0.3"
  }
}
